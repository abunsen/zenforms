<!DOCTYPE html>
<html>
<head>
  <title>Zenforms</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
  <% if alert || notice %>
    <div class="alert alert-<%= alert ? 'warning' : 'success' %>">
        <%= alert || notice %>
    </div>
  <% end %>
  <%= yield %>
</div>

</body>
</html>
